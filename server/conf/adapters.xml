<adapters_conf id="NODEJS_PERF_TEST">

	<metadata_provider>
		<adapter_class>com.lightstreamer.adapters.remote.metadata.RobustNetworkedMetadataProvider</adapter_class>
		<classloader>log-enabled</classloader>
		<param name="request_reply_port">12003</param>
		<param name="timeout">36000000</param>
	</metadata_provider>

	<data_provider>
		<adapter_class>com.lightstreamer.adapters.remote.data.RobustNetworkedDataProvider</adapter_class>
		<classloader>log-enabled</classloader>
		<param name="request_reply_port">12001</param>
		<param name="notify_port">12002</param>
		<param name="timeout">36000000</param>
	</data_provider>

</adapters_conf>
